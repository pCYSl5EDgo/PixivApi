{
  "$id": "https://raw.githubusercontent.com/pCYSl5EDgo/PixivApi/main/PixivApi.Core/UserFilterSchema.json",
  "$schema": "https://json-schema.org/draft-07/schema",
  "additionalProperties": false,
  "description": "A filter for user json file",
  "properties": {
    "$schema": {
      "format": "uri",
      "type": "string"
    },
    "follow": {
      "type": "boolean"
    },
    "id-filter": {
      "additionalProperties": false,
      "properties": {
        "id": {
          "items": {
            "minimum": 0,
            "type": "integer"
          },
          "minItems": 1,
          "type": "array",
          "uniqueItems": true
        },
        "ignore-id": {
          "items": {
            "minimum": 0,
            "type": "integer"
          },
          "minItems": 1,
          "type": "array",
          "uniqueItems": true
        }
      },
      "type": "object"
    },
    "name-filter": {
      "additionalProperties": false,
      "description": "A filter for text json file",
      "properties": {
        "exact": {
          "items": {
            "type": "string"
          },
          "minItems": 1,
          "type": "array",
          "uniqueItems": true
        },
        "exact-or": {
          "type": "boolean"
        },
        "ignore-exact": {
          "$ref": "#/properties/name-filter/properties/exact"
        },
        "ignore-exact-or": {
          "type": "boolean"
        },
        "ignore-partial": {
          "$ref": "#/properties/name-filter/properties/exact"
        },
        "ignore-partial-or": {
          "type": "boolean"
        },
        "partial": {
          "$ref": "#/properties/name-filter/properties/exact"
        },
        "partial-or": {
          "type": "boolean"
        }
      },
      "title": "TextFilter",
      "type": "object"
    },
    "only-registered": {
      "type": "boolean"
    },
    "show-hidden": {
      "type": "boolean"
    },
    "tag-filter": {
      "additionalProperties": false,
      "description": "A filter for text json file",
      "properties": {
        "exact": {
          "items": {
            "type": "string"
          },
          "minItems": 1,
          "type": "array",
          "uniqueItems": true
        },
        "exact-or": {
          "type": "boolean"
        },
        "ignore-exact": {
          "$ref": "#/properties/tag-filter/properties/exact"
        },
        "ignore-exact-or": {
          "type": "boolean"
        },
        "ignore-partial": {
          "$ref": "#/properties/tag-filter/properties/exact"
        },
        "ignore-partial-or": {
          "type": "boolean"
        },
        "partial": {
          "$ref": "#/properties/tag-filter/properties/exact"
        },
        "partial-or": {
          "type": "boolean"
        }
      },
      "title": "TextFilter",
      "type": "object"
    }
  },
  "title": "UserFilter",
  "type": "object"
}