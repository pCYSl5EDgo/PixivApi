{
  "$id": "https://raw.githubusercontent.com/pCYSl5EDgo/PixivApi/main/PixivApi.Core/TextFilterSchema.json",
  "$schema": "https://json-schema.org/draft-07/schema",
  "additionalProperties": false,
  "description": "A filter for text json file",
  "properties": {
    "$schema": {
      "format": "uri",
      "type": "string"
    },
    "exact": {
      "items": {
        "type": "string"
      },
      "minItems": 1,
      "type": "array",
      "uniqueItems": true
    },
    "exact-or": {
      "type": "boolean"
    },
    "ignore-exact": { "$ref": "#/properties/exact" },
    "ignore-exact-or": {
      "type": "boolean"
    },
    "ignore-partial": { "$ref": "#/properties/exact" },
    "ignore-partial-or": {
      "type": "boolean"
    },
    "partial": { "$ref": "#/properties/exact" },
    "partial-or": {
      "type": "boolean"
    }
  },
  "title": "TextFilter",
  "type": "object"
}